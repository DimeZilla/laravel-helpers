{
    "name": "dimezilla/laravel-helpers",
    "description": "A quick package to store some helper functions I'm using in laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Diamond",
            "email": "diamond.joshh@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "DiamondLaravelHelpers\\": "src/"
        }
    },
    "require-dev": {
        "dimezilla/phpdoc-github-markdown": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DiamondLaravelHelpers\\Providers\\ViewProvider"
            ],
            "aliases": {
                "ViewHelpers": "DiamondLaravelHelpers\\Facades\\ViewHelpers"
            }
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/DimeZilla/phpdoc-github-markdown.git"
        }
    ]
}
